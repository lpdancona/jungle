<h1>Categories</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Created At</th>
      <th>Updated At</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr class="category-list">
        <td><%= category.name %></td>
        <td><%= category.created_at %></td>
        <td><%= category.updated_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to "New Category", new_admin_category_path, class: 'new-cat'  %>